{
  'printWidth': 100,
  'singleQuote': true,
  'tabWidth': 2,
  'useTabs': false,
  'trailingComma': 'none',
  'bracketSpacing': true,
  'jsxBracketSameLine': false,
  'arrowParens': 'avoid',
  'overrides': [
    {
      'files': '*.scss',
      'options': {
        'useTabs': false,
        'tabWidth': 2
      }
    },
    {
      'files': 'package.json',
      'options': {
        'useTabs': false,
        'tabWidth': 2
      }
    }
  ]
}
